{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "jasmine": true,
    "jquery": true
  },
  "globals": {
    "google": true,
    "CSHMap": true
  },
  "rules": {
    "max-len": [2, 120],
    "new-cap": 1,
    "no-useless-constructor": 1,
    "no-underscore-dangle": 0,
    "padded-blocks": 0
  }
}
